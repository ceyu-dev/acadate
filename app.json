{
  "expo": {
    "name": "Acadate",
    "slug": "acadate",
    "version": "0.2.1-alpha",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#685ab3",
    "splash": {
      "image": "./assets/images/acadate-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#685ab3"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yuansdev.acadate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b075cc94-ad98-4540-8fe8-92408c256213"
      }
    }
  }
}
